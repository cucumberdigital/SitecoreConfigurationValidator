<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="sitecoreRoles" type="SitecoreConfigurationValidator.Xml.SitecoreRoleSection, SitecoreConfigurationValidator" />
    <section name="spreadsheetExclusions" type="SitecoreConfigurationValidator.Xml.SpreadsheetExclusionsSection, SitecoreConfigurationValidator" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="Excel.HeaderRowIndex" value="6"/>
  </appSettings>

  <sitecoreRoles>
    <roles>
      <role name="Content Delivery (CD)" columnIndex="6"/>
      <role name="Content Management (CM)" columnIndex="7"/>
      <role name="Processing" columnIndex="8"/>
      <role name="CM + Processing" columnIndex="9"/>
      <role name="Reporting" columnIndex="10"/>
      
    </roles>
  </sitecoreRoles>
  <spreadsheetExclusions>
    <exclusions>
      <!--<exclude sitecoreRole="Content Delivery (CD)" action="Disable" fileName="Sitecore.Analytics.Tracking.Aggregation.config" />
      <exclude sitecoreRole="Content Delivery (CD)" action="Enable" fileName="Sitecore.FXM.Lucene.DomainsSearch.DefaultIndexConfiguration.config" />-->
    </exclusions>
  </spreadsheetExclusions>
  
</configuration>